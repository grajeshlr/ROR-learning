<h1> Users </h1>

  


<tr>
<h3><%= button_to 'create', new_user_path,method: 'get' %></h3>

  <% @users.each do |user| %>
    <h4><%= link_to user.name, user_path(user)  %></h4>
    <tr><%= button_to 'edit', edit_user_path(user),method: 'get'%></tr>
    <tr><%= button_to 'delete', user_path(user),method: :delete, data: {confirm: 'Are you sure?'} %></tr>

    <tr><%= button_to 'show', user_path(user),method: 'get'%></tr>
  <% end %>
  
</tr>